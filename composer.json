{
    "name": "rundum/event-base",
    "description": "Basic events for symfony projects",
    "type": "library",
    "authors": [
        {
            "name": "Hendrik Pilz",
            "email": "pilz@rundum.digital"
        }
    ],
    "license": "MIT",
    "require": {
        "doctrine/orm": "^2.7",
        "psr/log": "^1.1 || ^2.0",
        "symfony/event-dispatcher": "^4.3 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Rundum\\": "src/"
        }
    }
}
